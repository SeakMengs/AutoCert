meta {
  name: Login with email & pw
  type: http
  seq: 1
}

post {
  url: {{url}}/api/v1/auth/login/email
  body: multipartForm
  auth: none
}

body:multipart-form {
  email: examplezz@gmail.com
  password: 1234567
}

docs {
  	Required body model
    
    type LoginBody struct {
  		Email    string `json:"email" form:"email" binding:"required"`
  		Password string `json:"password" form:"password" binding:"required"`
  	}
}
